{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@common/*": [
        "src/common/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@util/*": [
        "src/util/*"
      ],
      "@data/*": [
        "src/data/*"
      ],
      "@icons/*": [
        "src/icons/*"
      ],
      "@forms/*": [
        "src/packages/forms/src/*",
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  },
  "include": [
    "src"
  ]
}